extends ../layout
block meta
	link(rel='stylesheet', href='/css/index.css')
block content
	.w-1200
		h1= title
		form(action='/',method='post')
			.label
				span 用户名
				input(type='text',name='username',max-length='20')
			.label
				span 密码
				input(type='password',name='pwd',min-length='6')
			.label
				input(type='submit',value='提交')
				input(type='reset',value='重置')